{
  "name": "bootstrap-4.3.1-iso",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "css": "npm-run-all css-compile css-prefix css-minify",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/bootstrap-iso.scss dist/css/bootstrap-iso.css",
    "css-prefix": "postcss --config src/postcss.config.js --replace dist/css/*.css",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/css/bootstrap-iso.min.css dist/css/bootstrap-iso.css"
  },
  "devDependencies": {
    "bootstrap": "^4.0.0-beta",
    "autoprefixer": "^7.2.5",
    "clean-css-cli": "^4.1.10",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1"
  }
}
